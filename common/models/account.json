{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "year": {
      "type": "number"
    },
    "isStaff": {
      "type": "boolean"
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "number"
    }
  },
  "hidden": [
    "email"
  ],
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": ""
    },
    "chats": {
      "type": "hasMany",
      "model": "Chat",
      "foreignKey": "",
      "through": "AccountChat"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "__get__course",
        "__get__chats",
        "__create__chats",
        "__unlink__chats"
      ]
    }
  ],
  "methods": {}
}
