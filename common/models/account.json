{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "year": {
      "type": "number"
    },
    "isStaff": {
      "type": "boolean"
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": ""
    },
    "chats": {
      "type": "hasMany",
      "model": "Chat",
      "foreignKey": "",
      "through": "AccountChat"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
