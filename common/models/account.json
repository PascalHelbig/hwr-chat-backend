{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "year": {
      "type": "number"
    },
    "isStaff": {
      "type": "boolean"
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "number"
    }
  },
  "hidden": [
    "email"
  ],
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": ""
    },
    "chats": {
      "type": "hasMany",
      "model": "Chat",
      "foreignKey": "",
      "through": "AccountChat"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__course"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__chats"
    }
  ],
  "methods": {}
}
